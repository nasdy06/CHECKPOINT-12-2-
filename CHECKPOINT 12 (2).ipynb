    "import time\n",
    "t=int(input(\"Duree du compte a rebours : \"))\n",
    "def countdown(t):\n",
    "    while t>0:\n",
    "        m,s=divmod(t,60)\n",
    "        print(\"{:02d}:{:02d}\".format(m,s),end='\\r')\n",
    "        time.sleep(1)\n",
    "        t-=1\n",
    "    print(\"Fire in the hole\")\n",
    "countdown(t)\n",
    "    "
   ]
